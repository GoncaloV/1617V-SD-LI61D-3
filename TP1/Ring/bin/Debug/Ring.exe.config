<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <system.runtime.remoting>
    <application>
      <lifetime leaseTime="0" renewOnCallTime="2S" leaseManagerPollTime="1S"/>
      <channels>
        <channel ref="http" port="1234">
          <clientProviders>
            <formatter ref="binary"/>
          </clientProviders>
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full"/>
          </serverProviders>
        </channel>
      </channels>
      <service>
        <!-- Namespace.Type, Assembly  -->
        <wellknown type="ManagerClass.ManagerClassImpl, RingAndKeyManager"
                   mode="Singleton"
                   objectUri="RemServer.rem"/>
      </service>
    </application>
  </system.runtime.remoting>

  <appSettings>
    <add key="serverNumber" value="1"/>
    <add key="1" value="http://localhost:1234/RemServer.rem" />
    <add key="2" value="http://localhost:1235/RemServer.rem" />
    <add key="3" value="http://localhost:1236/RemServer.rem" />
    <add key="4" value="http://localhost:1237/RemServer.rem" />
    <add key="5" value="http://localhost:1238/RemServer.rem" />
    <add key="6" value="http://localhost:1239/RemServer.rem" />
  </appSettings>

</configuration>