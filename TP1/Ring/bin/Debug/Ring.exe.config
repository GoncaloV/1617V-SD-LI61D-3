<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <system.runtime.remoting>
    <application>
      <lifetime leaseTime="0" renewOnCallTime="2S" leaseManagerPollTime="1S"/>
      <channels>
        <channel ref="http" port="1234">
          <clientProviders>
            <formatter ref="binary"/>
          </clientProviders>
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full"/>
          </serverProviders>
        </channel>
      </channels>
      <service>
        <!-- Namespace.Type, Assembly  -->
        <wellknown type="ManagerClass.ManagerClassImpl, RingAndKeyManager"
                   mode="Singleton"
                   objectUri="RemServer.rem"/>
      </service>
      <service>
        <!-- Namespace.Type, Assembly  -->
        <wellknown type="ServerClass.ServerClassImpl, ServerClass"
                   mode="Singleton"
                   objectUri="Server1.rem"/>
      </service>
      <service>
        <!-- Namespace.Type, Assembly  -->
        <wellknown type="ServerClass.ServerClassImpl, ServerClass"
                   mode="Singleton"
                   objectUri="Server2.rem"/>
      </service>
      <service>
        <!-- Namespace.Type, Assembly  -->
        <wellknown type="ServerClass.ServerClassImpl, ServerClass"
                   mode="Singleton"
                   objectUri="Server3.rem"/>
      </service>
  <service>
        <!-- Namespace.Type, Assembly  -->
        <wellknown type="ServerClass.ServerClassImpl, ServerClass"
                   mode="Singleton"
                   objectUri="Server4.rem"/>
      </service>
      <service>
        <!-- Namespace.Type, Assembly  -->
        <wellknown type="ServerClass.ServerClassImpl, ServerClass"
                   mode="Singleton"
                   objectUri="Server5.rem"/>
      </service>
    </application>
  </system.runtime.remoting>

  <appSettings>
    <add key="1" value="http://localhost:1234/RemServer.rem" />
    <add key="2" value="http://localhost:1234/Server1.rem" />
    <add key="3" value="http://localhost:1234/Server2.rem" />
    <add key="4" value="http://localhost:1234/Server3.rem" />
    <add key="5" value="http://localhost:1234/Server4.rem" />
    <add key="6" value="http://localhost:1234/Server5.rem" />
  </appSettings>

</configuration>